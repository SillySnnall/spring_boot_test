<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.ruolan.springtest.spring_data_jpa.User">
        <table name="user" schema="myself" catalog=""/>
        <attributes>
            <id name="id">
                <column name="_id" precision="11"/>
            </id>
            <basic name="account">
                <column name="account" nullable="false" length="50"/>
            </basic>
            <basic name="password">
                <column name="password" nullable="false" length="50"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" length="20"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
